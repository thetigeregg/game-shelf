{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/manifest.webmanifest",
          "/assets/icon/favicon.png",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-read-cache",
      "urls": [
        "/api/v1/games/**",
        "/api/v1/platforms",
        "/api/v1/popularity/**",
        "/api/v1/images/boxart/search**",
        "/api/v1/manuals/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 500,
        "maxAge": "1d"
      }
    },
    {
      "name": "api-images-proxy-cache",
      "urls": [
        "/api/v1/images/proxy**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 1000,
        "maxAge": "7d"
      }
    }
  ]
}
